{
	"name": "InstancesSupervisor",
	"endpoints": [
		{
			"summary": "Overview",
			"method": "GET",
			"path": "/overview"
		},
		{
			"summary": "ListSockets",
			"method": "GET",
			"path": "/sockets"
		},
		{
			"summary": "InstanceSocketFileListChange",
			"method": "WS",
			"path": "/instance-socket-file-list-change"
		},
		{
			"summary": "ShowInstanceStatus",
			"method": "GET",
			"path": "/socket/:socketFileName/status",
			"parameters": [
				{
					"name": "socketFileName",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"summary": "ListInstanceTasks",
			"method": "GET",
			"path": "/socket/:socketFileName/tasks",
			"parameters": [
				{
					"name": "socketFileName",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"summary": "KillInstance",
			"method": "POST",
			"path": "/kill-instance",
			"parameters": [
				{
					"name": "socketFileName",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"summary": "GetTaskInformation",
			"method": "GET",
			"path": "/socket/:socketFileName/task/:taskId",
			"parameters": [
				{
					"name": "socketFileName",
					"in": "path",
					"type": "string",
					"required": true
				},
				{
					"name": "taskId",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
		}
	]
}