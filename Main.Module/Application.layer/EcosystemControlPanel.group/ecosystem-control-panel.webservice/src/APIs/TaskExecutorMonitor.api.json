{
  "name": "TaskExecutorMonitor",
  "endpoints": [
    {
      "summary": "GetMonitoringState",
      "method": "GET",
      "path": "/monitoring/state"
    },
    {
      "summary": "MonitoringState",
      "method": "WS",
      "path": "/monitoring"
    },
    {
      "summary": "ListTasks",
      "method": "GET",
      "path": "/list-task"
    },
    {
      "summary": "TaskList",
      "method": "WS",
      "path": "/task-list"
    },
    {
      "summary": "GetTaskTreeById",
      "method": "POST",
      "path": "/task-tree-by-id",
      "parameters": [
				{
					"name": "taskId",
					"in": "body",
					"type": "string",
					"required": true
				}
      ]
    },
    {
      "summary": "GetTaskInformation",
      "method": "POST",
      "path": "/task/information",
      "parameters": [
				{
					"name": "taskId",
					"in": "body",
					"type": "string",
					"required": true
				}
      ]
    }
  ]
}