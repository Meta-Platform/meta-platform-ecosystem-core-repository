{
    "params": ["installDataDirPath", "ecosystemDefaultsFileRelativePath"],
    "bound-params":[
        "serverService", 
        "taskExecutorMachineService",
        "repositoryManagerService",
        "ecosystemManagerService",
        "instanceMonitoringService",
        "supervisorLib",
        "ecosystemControlPanelService",
        "jsonFileUtilitiesLib"
    ],
    "endpoints": [
        {
            "url": "/task-executor-monitor",
            "type": "controller",
            "params": {
                "api-template": "APIs/TaskExecutorMonitor.api.json",
                "controller": "Controllers/TaskExecutorMonitor.controller"
            },
            "bound-params": {
                "controller-params":{
                    "taskExecutorMachineService":"taskExecutorMachineService"
                },
                "serverService":"serverService"
            }
        },
        {
            "url": "/repository-manager",
            "type": "controller",
            "params": {
                "api-template": "APIs/RepositoryManager.api.json",
                "controller": "Controllers/RepositoryManager.controller"
            },
            "bound-params": {
                "controller-params":{
                    "repositoryManagerService":"repositoryManagerService"
                },
                "serverService":"serverService"
            }
        },
        {
            "url": "/ecosystem-manager",
            "type": "controller",
            "params": {
                "api-template": "APIs/EcosystemManager.api.json",
                "controller": "Controllers/EcosystemManager.controller"
            },
            "bound-params": {
                "controller-params":{
                    "ecosystemManagerService":"ecosystemManagerService",
                    "repositoryManagerService": "repositoryManagerService"
                },
                "serverService":"serverService"
            }
        },
        {
            "url": "/supervisor",
            "type": "controller",
            "params": {
                "api-template": "APIs/InstancesMonitor.api.json",
                "controller": "Controllers/InstancesMonitor.controller",
                "controller-params":{
                    "installDataDirPath": "{{installDataDirPath}}",
                    "ecosystemDefaultsFileRelativePath": "{{ecosystemDefaultsFileRelativePath}}"
                }
            },
            "bound-params": {
                "serverService":"serverService",
                "controller-params":{
                    "instanceMonitoringService":"instanceMonitoringService",
                    "supervisorLib":"supervisorLib",
                    "jsonFileUtilitiesLib": "jsonFileUtilitiesLib"
                }
            }
        },
        {
            "url": "/applications-and-repositories",
            "type": "controller",
            "params": {
                "api-template": "APIs/ApplicationsAndRepositories.api.json",
                "controller": "Controllers/ApplicationsAndRepositories.controller",
                "controller-params":{
                    "installDataDirPath": "{{installDataDirPath}}",
                    "ecosystemDefaultsFileRelativePath": "{{ecosystemDefaultsFileRelativePath}}"
                }
            },
            "bound-params": {
                "serverService":"serverService",
                "controller-params":{
                    "jsonFileUtilitiesLib": "jsonFileUtilitiesLib"
                }
            }
        },
        {
            "url": "/environments",
            "type": "controller",
            "params": {
                "api-template": "APIs/Environments.api.json",
                "controller": "Controllers/Environments.controller"
            },
            "bound-params": {
                "serverService":"serverService",
                "controller-params":{
                    "ecosystemControlPanelService": "ecosystemControlPanelService"
                }
            }
        },
        {
            "url": "/sources",
            "type": "controller",
            "params": {
                "api-template": "APIs/Sources.api.json",
                "controller": "Controllers/Sources.controller"
            },
            "bound-params": {
                "serverService":"serverService"
            }
        },
        {
            "url": "/configurations",
            "type": "controller",
            "params": {
                "api-template": "APIs/Configurations.api.json",
                "controller": "Controllers/Configurations.controller"
            },
            "bound-params": {
                "serverService":"serverService"
            }
        }
    ]
}