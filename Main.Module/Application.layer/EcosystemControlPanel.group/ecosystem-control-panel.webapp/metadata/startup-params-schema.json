{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "port": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "serverName": {
      "type": "string"
    },
    "serverManagerUrl": {
      "type": "string",
      "format": "uri"
    },
    "packagePath": {
      "type": "string"
    },
    "RT_ENV_GENERATED_DIR_NAME": {
      "type": "string"
    },
    "supervisorSocketsDirPath": {
      "type": "string"
    }
  },
  "required": [
    "port",
    "serverName",
    "serverManagerUrl",
    "packagePath",
    "RT_ENV_GENERATED_DIR_NAME",
    "supervisorSocketsDirPath"
  ]
}
