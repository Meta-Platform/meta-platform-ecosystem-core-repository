{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "port": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "serverName": {
      "type": "string"
    },
    "serverManagerUrl": {
      "type": "string",
      "format": "uri"
    },
    "packagePath": {
      "type": "string"
    },
    "RT_ENV_GENERATED_DIR_NAME": {
      "type": "string"
    },
    "installDataDirPath": {
      "type": "string"
    },
    "ecosystemDefaultsFileRelativePath":{
      "type": "string"
    },
    "REPOS_CONF_FILENAME_REPOS_DATA":{
      "type": "string"
    },
    "REPOS_CONF_EXT_MODULE_DIR":{
      "type": "string"
    },
    "REPOS_CONF_EXT_LAYER_DIR":{
      "type": "string"
    },
    "REPOS_CONF_EXT_GROUP_DIR":{
      "type": "string"
    },
    "REPOS_CONF_EXTLIST_PKG_TYPE":{
      "type": "string"
    },
    "PKG_CONF_DIRNAME_METADATA":{
      "type": "string"
    }
  },
  "required": [
    "port",
    "serverName",
    "serverManagerUrl",
    "packagePath",
    "RT_ENV_GENERATED_DIR_NAME",
    "installDataDirPath",
    "ecosystemDefaultsFileRelativePath",
    "REPOS_CONF_FILENAME_REPOS_DATA",
    "REPOS_CONF_EXT_MODULE_DIR",
    "REPOS_CONF_EXT_LAYER_DIR",
    "REPOS_CONF_EXT_GROUP_DIR",
    "REPOS_CONF_EXTLIST_PKG_TYPE",
    "PKG_CONF_DIRNAME_METADATA"
  ]
}
