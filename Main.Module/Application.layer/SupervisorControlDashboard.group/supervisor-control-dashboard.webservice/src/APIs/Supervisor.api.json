{
	"name": "Supervisor",
	"endpoints": [
		{
			"summary": "ListSockets",
			"method": "GET",
			"path": "/sockets"
		},
		{
			"summary": "ShowInstanceStatus",
			"method": "GET",
			"path": "/socket/:socketFilename/status",
			"parameters": [
				{
					"name": "socketFilename",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"summary": "ListInstanceTasks",
			"method": "GET",
			"path": "/socket/:socketFilename/tasks",
			"parameters": [
				{
					"name": "socketFilename",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"summary": "KillInstance",
			"method": "POST",
			"path": "/kill-instance",
			"parameters": [
				{
					"name": "socketFilename",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"summary": "ShowInstanceTaskInformation",
			"method": "GET",
			"path": "/socket/:socketFilename/task/:taskId",
			"parameters": [
				{
					"name": "socketFilename",
					"in": "path",
					"type": "string",
					"required": true
				},
				{
					"name": "taskId",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
		}
	]
}